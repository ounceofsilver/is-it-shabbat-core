{
	"name": "is-it-shabbat-core",
	"version": "1.0.2",
	"main": "lib/index.js",
	"licenses": "MIT",
	"author": "James Fulford <james.fulford@outlook.com>",
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint",
			"pre-push": "npm run test && npm run build"
		}
	},
	"nyc": {
		"include": "src",
		"exclude": "**/*.spec.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "mocha ./src/**/*.spec.js",
		"tdd": "npm run test -- -w",
		"lint": "eslint ./src/**/*.js ./test",
		"build": "babel src -d lib",
		"prepare": "npm run build",
		"report": "nyc --reporter=lcov --reporter=text npm run test",
		"coverage": "npm run report && open ./coverage/lcov-report/index.html"
	},
	"dependencies": {
		"axios": "0.18.0",
		"luxon": "1.8.0",
		"prop-types": "15.6.2",
		"react": "16.7.0",
		"redux": "4.0.0",
		"shabbat-logic": "1.0.0",
		"tz-lookup": "6.1.8"
	},
	"devDependencies": {
		"@babel/cli": "7.2.3",
		"@babel/core": "7.2.2",
		"@babel/plugin-proposal-export-default-from": "7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "7.2.0",
		"@babel/polyfill": "7.2.5",
		"@babel/preset-env": "7.2.3",
		"@babel/preset-react": "7.0.0",
		"@babel/register": "7.0.0",
		"babel-eslint": "10.0.1",
		"chai": "4.2.0",
		"chai-enzyme": "1.0.0-beta.1",
		"dirty-chai": "2.0.1",
		"enzyme": "3.8.0",
		"enzyme-adapter-react-16": "1.7.1",
		"eslint": "5.9.0",
		"eslint-config-airbnb": "17.1.0",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jsx-a11y": "6.1.2",
		"eslint-plugin-react": "7.11.1",
		"husky": "1.2.1",
		"jsdom": "13.1.0",
		"mocha": "5.2.0",
		"mocha-eslint": "4.1.0",
		"nyc": "13.1.0",
		"proxyquire": "2.1.0",
		"react-dom": "16.7.0",
		"sinon": "7.1.1",
		"sinon-chai": "3.2.0"
	}
}
